extends ../../layouts/default2.pug
block main
    .app-content.content
      .content-wrapper
        .content-wrapper-before(style="background-color:#32cafe")
        .content-header.row(style="margin-top:200px;")
          .content-header-left.col-md-4.col-12.mb-2
            h3.content-header-title Post
          .content-header-right.col-md-8.col-12
            .breadcrumbs-top.float-md-right
              .breadcrumb-wrapper.mr-1
                ol.breadcrumb
                  li.breadcrumb-item
                    a(href='/ad/dashboard') Home
                  li.breadcrumb-item.active
                    |Posts
        .content-body
          // ../../../theme-assets/images/carousel/22.jpg
          // Header footer section start
          section#header-footer
            .row.match-height
              each item in posts
                .col-lg-4.col-md-12
                  .card
                    .card-body
                      h4.card-title Newest posts
                      h6.card-subtitle.text-muted #{item.title }
                      h6.card-subtitle.text-muted uploaded by #{item.userName}
                    img(src='/' +item.image alt='Card image cap')
                    .card-body
                      p.card-text
                        | #{item.content}
                      a.card-link(href='#') Card link
                      a.card-link(href='#') Another link
                    .card-footer.border-top-blue-grey.border-top-lighten-5.text-muted
                      span.float-left #{moment(item.DateAt).fromNow()}
                      span.float-right
                        a.card-link(href='#')
                          | Read More
                          i.la.la-angle-right
